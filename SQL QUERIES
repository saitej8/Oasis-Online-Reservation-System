-- create database
create database online_reservation_system;

-- use database
use online_reservation_system;

-- create users table--
create table users(userid int primary key auto_increment,username varchar(50),password varchar(50),role varchar(20));

-- create reservations table--

create table reservations(reservationId int primary key auto_increment,pnrNumber varchar(20),passengerName varchar(100),trainNo varchar(10),trainName varchar(50),classType varchar(20),date date,fromPlace varchar(50),toPlace varchar(50));




